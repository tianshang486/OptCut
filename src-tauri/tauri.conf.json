{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauriDemo",
  "version": "0.1.0",
  "identifier": "com.Demo.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tauriDemo",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "capabilities": [
        "default"
      ],
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$APPDATA/**",
          "$TEMP/**"
        ]
      },
      "csp": "default-src 'self'; img-src 'self' asset: http://asset.localhost https: data:;"
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "tools/**/*"
    ]
  }
}

